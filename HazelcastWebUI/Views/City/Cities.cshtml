@model IEnumerable<Model.City>

<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">City</th>
            <th scope="col">Country</th>
        </tr>
    </thead>
    <tbody>
        @{
            int i = 1;
        }
        @foreach (var item in Model)
        {
            <tr>
                <th scope="row">@i</th>
                <td>@item.CityName</td>
                <td>@item.CountryName</td>
            </tr>
            i++;
        }
    </tbody>
</table>
